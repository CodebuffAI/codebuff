{
  "name": "common",
  "version": "1.0.0",
  "description": "Common utilities for Codebuff",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "bun run clean",
    "build": "tsc",
    "typecheck-this-package": "tsc --noEmit",
    "typecheck": "bun run typecheck-this-package",
    "clean": "rm -rf dist && (rm tsconfig.tsbuildinfo || true)",
    "db:generate": "infisical run --log-level=warn -- drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "nx run common:db-migrate",
    "db:start": "docker compose -f ./src/db/docker-compose.yml up --wait && bun run db:generate && (timeout 1 || sleep 1) && bun run db:migrate",
    "db:studio": "infisical run --log-level=warn -- drizzle-kit studio --config=./src/db/drizzle.config.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.5.0",
    "@types/pg": "^8.11.10",
    "@types/readable-stream": "^4.0.18",
    "@types/seedrandom": "^3.0.8",
    "drizzle-kit": "0.28.1",
    "drizzle-orm": "0.36.4",
    "ignore": "5.3.2",
    "lodash": "*",
    "next-auth": "^4.24.7",
    "pg": "^8.14.1",
    "readable-stream": "^4.7.0",
    "seedrandom": "^3.0.5",
    "stripe": "^16.11.0"
  },
  "peerDependencies": {
    "@types/node": "*",
    "@types/lodash": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/parse-path": "^7.1.0"
  }
}