    "pg": "^8.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
    "drizzle-kit": "^0.24.2",
    "typescript": "^5"
  }
}
