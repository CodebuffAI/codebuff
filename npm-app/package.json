{
  "name": "codebuff",
  "version": "1.0.324",
  "description": "AI dev assistant",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": "./src/*.ts"
  },
  "bin": {
    "codebuff": "dist/index.js"
  },
  "scripts": {
    "prepublishOnly": "bun run bundle && infisical run --env=prod --log-level=warn -- bun run clean-package.js",
    "postpublish": "echo \"const fs=require('fs');fs.copyFileSync('temp.package.json','package.json');fs.unlinkSync('temp.package.json');\" | bun run -",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "jest",
    "start-bin": "infisical run --log-level=warn -- bun run build-binary && ./bin/codebuff ..",
    "start": "infisical run --log-level=warn -- bun run src/index.ts ..",
    "start-max": "infisical run --log-level=warn -- bun run src/index.ts .. --max",
    "start-lite": "infisical run --log-level=warn -- bun run src/index.ts .. --lite",
    "start-ask": "infisical run --log-level=warn -- bun run src/index.ts .. --ask",
    "start-superagent": "infisical run --log-level=warn -- bun run src/index.ts .. --superagent",
    "start-dir": "infisical run --log-level=warn -- bun run src/index.ts",
    "start-experimental": "infisical run --log-level=warn -- bun run src/index.ts .. --experimental",
    "start-manifold": "infisical run --log-level=warn -- bun run src/index.ts ../../manifold",
    "start-litestar": "infisical run --log-level=warn -- bun run src/index.ts ../test/__mock-projects__/litestar",
    "start-jpcsp": "infisical run --log-level=warn -- bun run src/index.ts ../test/__mock-projects__/jpcsp",
    "start-vscode": "infisical run --log-level=warn -- bun run src/index.ts ../test/__mock-projects__/vscode",
    "start-nushell": "infisical run --log-level=warn -- bun run src/index.ts ../test/__mock-projects__/nushell",
    "typecheck": "tsc --noEmit -p .",
    "build-binary": "node scripts/build-binaries.js",
    "build-all-binaries": "node scripts/build-all-platforms.js",
    "build": "bun run build-all-binaries"
  },
  "files": [
    "README.md"
  ],
  "engines": {
    "bun": ">=1.2.11"
  },
  "dependencies": {
    "@types/diff": "5.2.1",
    "@types/micromatch": "^4.0.9",
    "@vscode/ripgrep": "1.15.9",
    "ai": "4.3.16",
    "axios": "1.7.4",
    "@codebuff/code-map": "workspace:*",
    "@codebuff/common": "workspace:*",
    "commander": "^13.1.0",
    "diff": "5.2.0",
    "git-url-parse": "^16.1.0",
    "ignore": "7.0.3",
    "isomorphic-git": "^1.29.0",
    "lodash": "*",
    "micromatch": "^4.0.8",
    "nanoid": "5.0.7",
    "onetime": "5.1.2",
    "picocolors": "1.1.0",
    "pino": "9.4.0",
    "posthog-node": "4.17.2",
    "puppeteer-core": "^24.2.0",
    "systeminformation": "5.23.4",
    "ts-pattern": "5.3.1",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "optionalDependencies": {
    "@homebridge/node-pty-prebuilt-multiarch": "0.12.0"
  },
  "trustedDependencies": [
    "@homebridge/node-pty-prebuilt-multiarch",
    "@vscode/ripgrep"
  ]
}
