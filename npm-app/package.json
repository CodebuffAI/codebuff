{
  "name": "codebuff",
  "version": "1.0.119",
  "description": "AI dev assistant",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codebuff": "dist/index.js"
  },
  "scripts": {
    "prebuild": "bun clean && cd ../common && bun run build && cd ../packages/code-map && bun run build",
    "build": "tsc && tsc-alias",
    "postbuild": "mkdir -p dist/common && cp -r ../common/dist/* dist/common && mkdir -p dist/code-map && cp -r ../packages/code-map/dist/* dist/code-map && tsc-alias",
    "clean": "rm -rf dist && (rm tsconfig.tsbuildinfo || true)",
    "prepublishOnly": "bun run build && bun run clean-package.js",
    "postpublish": "echo \"const fs=require('fs');fs.copyFileSync('temp.package.json','package.json');fs.unlinkSync('temp.package.json');\" | bun run -",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "start": "bun run build && ENVIRONMENT=local bun src/index.ts",
    "demo:menu": "ENVIRONMENT=local bun src/demo.tsx menu",
    "demo:progress": "ENVIRONMENT=local bun src/demo.tsx progress",
    "start-dir": "ENVIRONMENT=local bun src/index.ts",
    "start-manifold": "ENVIRONMENT=local bun src/index.ts ../../manifold",
    "start-litestar": "ENVIRONMENT=local bun src/index.ts ../test/__mock-projects__/litestar",
    "start-jpcsp": "ENVIRONMENT=local bun src/index.ts ../test/__mock-projects__/jpcsp",
    "start-vscode": "ENVIRONMENT=local bun src/index.ts ../test/__mock-projects__/vscode",
    "start-nushell": "ENVIRONMENT=local bun src/index.ts ../test/__mock-projects__/nushell",
    "start-prod": "ENVIRONMENT=local bun src/index.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/diff": "5.2.1",
    "axios": "1.7.4",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "cheerio": "1.0.0",
    "cli-table3": "^0.6.5",
    "diff": "5.2.0",
    "env-cmd": "^10.1.0",
    "ignore": "5.3.2",
    "ink": "3.2.0",
    "lodash": "*",
    "nanoid": "5.0.7",
    "picocolors": "1.1.0",
    "prompts": "^2.4.2",
    "react": "17.0.2",
    "systeminformation": "5.23.4",
    "tree-sitter": "0.21.1",
    "tree-sitter-c": "0.23.0",
    "tree-sitter-c-sharp": "0.23.0",
    "tree-sitter-cpp": "0.23.0",
    "tree-sitter-go": "0.23.1",
    "tree-sitter-java": "0.23.2",
    "tree-sitter-javascript": "0.23.0",
    "tree-sitter-php": "0.23.2",
    "tree-sitter-python": "0.23.2",
    "tree-sitter-ruby": "0.23.0",
    "tree-sitter-rust": "0.23.0",
    "tree-sitter-typescript": "0.23.0",
    "ts-pattern": "5.3.1",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/ink": "2.0.3",
    "@types/node": "^20.10.1",
    "@types/react": "17.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
