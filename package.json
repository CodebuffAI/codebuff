{
  "name": "codebuff-project",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "workspaces": [
    "common",
    "backend",
    "npm-app",
    "web",
    "packages/*",
    "scripts",
    "evals"
  ],
  "scripts": {
    "start-db": "bun --cwd common db:start",
    "start-client": "bun --cwd npm-app start",
    "start-bin": "bun --cwd npm-app start-bin",
    "start-experimental": "bun --cwd npm-app start-experimental",
    "start-max": "bun --cwd npm-app start-max",
    "start-lite": "bun --cwd npm-app start-lite",
    "start-ask": "bun --cwd npm-app start-ask",
    "start-superagent": "bun --cwd npm-app start-superagent",
    "start-web": "bun start-db && bun --cwd web dev",
    "start-studio": "bun --cwd common db:studio",
    "start-server": "bun --cwd backend dev",
    "start-manifold": "bun --cwd npm-app start-manifold",
    "start-codebuff-test": "bun --cwd npm-app start-dir ../../codebuff-test",
    "start-litestar": "bun --cwd npm-app start-litestar",
    "start-jpcsp": "bun --cwd npm-app start-jpcsp",
    "start-vscode": "bun --cwd npm-app start-vscode",
    "start-nushell": "bun --cwd npm-app start-nushell",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "release:npm-app": "bun run --cwd npm-app release",
    "typecheck": "bun --filter='*' run typecheck && echo 'âœ… All type checks passed!'",
    "test": "bun --filter='{@codebuff/backend,@codebuff/common,@codebuff/npm-app}' run test"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.7.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.59.16",
    "@types/bun": "^1.2.11",
    "@types/lodash": "4.17.7",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.12",
    "@types/parse-path": "^7.1.0",
    "bun-types": "^1.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "ignore": "^6.0.2",
    "lodash": "4.17.21",
    "prettier": "3.3.2",
    "ts-node": "^10.9.2",
    "ts-pattern": "^5.5.0",
    "tsc-alias": "1.7.0",
    "tsconfig-paths": "4.2.0",
    "types": "^0.1.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
