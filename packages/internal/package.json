{
  "name": "@codebuff/internal",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./mcp": {
      "bun": "./src/mcp/index.ts",
      "import": "./src/mcp/index.ts",
      "types": "./src/mcp/index.ts",
      "default": "./src/mcp/index.ts"
    },
    "./loops": {
      "bun": "./src/loops/index.ts",
      "import": "./src/loops/index.ts",
      "types": "./src/loops/index.ts",
      "default": "./src/loops/index.ts"
    },
    "./*": {
      "bun": "./src/*.ts",
      "import": "./src/*.ts",
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "postinstall": "bun run scripts/generate-mcp-tools.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": ">=1.2.11"
  },
  "dependencies": {
    "@codebuff/common": "workspace:*",
    "@upstash/context7-mcp": "^1.0.0",
    "duckduckgo-mcp-server": "^0.1.2",
    "drizzle-orm": "*",
    "loops": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/bun": "^1.2.11"
  }
}
