{
  "name": "@codebuff/sdk",
  "private": false,
  "access": "public",
  "version": "0.1.6",
  "description": "Official SDK for Codebuff â€” AI coding agent & framework",
  "license": "MIT",
  "type": "module",
  "main": "./dist/sdk/src/index.js",
  "types": "./dist/sdk/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk/src/index.d.ts",
      "import": "./dist/sdk/src/index.js",
      "default": "./dist/sdk/src/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare-dist": "node scripts/publish.js --dry-run",
    "publish-sdk": "node scripts/publish.js --public",
    "publish-dry-run": "node scripts/publish.js --dry-run",
    "prepublishOnly": "bun run clean && bun run build",
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "codebuff",
    "ai",
    "code-editing",
    "assistant",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codebuff/codebuff.git",
    "directory": "sdk"
  },
  "homepage": "https://codebuff.com",
  "bugs": {
    "url": "https://github.com/codebuff/codebuff/issues"
  },
  "dependencies": {
    "ai": "4.3.16",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/bun": "^1.2.11"
  }
}
