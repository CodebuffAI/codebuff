{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es2020",
    "composite": true,
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noImplicitReturns": true,
    "strict": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "paths": {
      "@/env": ["./env.ts"]
    }
  }
}
