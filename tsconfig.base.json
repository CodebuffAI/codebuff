{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "esnext",
    "composite": true,
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noImplicitReturns": true,
    "strict": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true
  }
}
